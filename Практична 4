#include <stdio.h> 
#include <math.h> 
 
int main() { 
    // Оголошення змінної для числа n 
    int n; 
 
    // Введення числа n 
    printf("Введіть натуральне число n: "); 
    scanf("%d", &n); 
 
    // Перевірка введеного числа 
    if (n < 1 || n > 150) { 
        printf("Невірне число n. Число n має бути в діапазоні від 1 до 150.\n"); 
        return 1; 
    } 
 
    // Ініціалізація змінної для кількості рівних дільників 
    int count = 0; 
 
    // Перевірка, чи є n ідеальним квадратом 
    int sqrt_n = (int)sqrt(n); 
    if (sqrt_n * sqrt_n == n) { 
        count = 1; // n має один рівний дільник 
    } 
 
    // Виведення кількості рівних дільників 
    printf("Кількість рівних дільників числа %d: %d\n", n, count); 
 
    return 0; 
}
